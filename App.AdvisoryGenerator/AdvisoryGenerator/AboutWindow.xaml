        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Height="27" Margin="698,151,0,0" VerticalAlignment="Top" Width="101" FontSize="18" Click="OKButton_Click"/>
        <TextBlock x:Name="FandomLink" HorizontalAlignment="Left" Height="20" Margin="10,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="243">
            <Hyperlink RequestNavigate="Hyperlink_RequestNavigate" NavigateUri="http://hypotheticalhurricanes.fandom.com">http://hypotheticalhurricanes.fandom.com</Hyperlink>
        </TextBlock>
    </Grid>
</Window>
