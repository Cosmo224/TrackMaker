<Window x:Class="Dano.AdvisoryGenerator.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dano.AdvisoryGenerator"
        mc:Ignorable="d"
        Title="About" Height="226.421" Width="838.719">
    <Grid >
        <TextBlock x:Name="Advisory" Text="Advisory" HorizontalAlignment="Left" Margin="10,-2,0,0" VerticalAlignment="Top" Height="64" Width="203" FontSize="48" FontFamily="Segoe UI Semibold"/>
